{
  "name": "Mongo DB Viewer",
  "version": "0.0.1",
  "description": "MERN app for viewing Mongo databases with very basic interface.",
  "keywords": "mongodb, mongo, database viewer, MERN",
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack --config webpack.config.js --mode development",
    "development": "nodemon",
    "test": "jest --no-cache"
  },
  "license": "MIT",
  "author": {
    "name": "kevin davies"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "mongodb": "^3.2.3",
    "morgan": "~1.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-json-tree": "^0.11.2",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  }
}
